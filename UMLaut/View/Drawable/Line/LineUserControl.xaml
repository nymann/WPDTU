<UserControl x:Class="UMLaut.View.Drawable.LineUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UMLaut.View.Drawable"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Line Stroke="Black" X1="{Binding StartX}" Y1="{Binding StartY}" X2="{Binding EndX}" Y2="{Binding EndY}" StrokeThickness="2"/>
</UserControl>
