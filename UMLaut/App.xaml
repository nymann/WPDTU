<Application x:Class="UMLaut.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:UMLaut"
             xmlns:vm="clr-namespace:UMLaut.ViewModel"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <vm:BooleanToVisibilityConverter x:Key="TrueToVisibleConverter" />
        <vm:BooleanToVisibilityConverter x:Key="FalseToVisibleConverter" IsReversed="True" />
    </Application.Resources>
</Application>
